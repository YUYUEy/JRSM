<!DOCTYPE html>
<html>
<head>
	<title>Example</title>
</head>
<body>
<p>
	<b>hello</b> world!
</p>
<ul>
	<li>111</li>
	<li>222</li>
</ul>
<div>
	<div>
		<h4>i am h4~<span>i am span!</span></h4>
	</div>
</div>

</body>
</html>
<script type="text/javascript">
	onload = function(){
		//遍历HTML元素下面的所有元素
		var root =document.getElementsByTagName('html');
		var iterator =document.createNodeIterator(root[0],NodeFilter.SHOW_ELEMENT,null, false);
		var node = iterator.nextNode();
		while(node != null){
			console.log(node.tagName);
			node = iterator.nextNode();
		}
		debugger
	}
</script>
